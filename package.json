    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@primevue/auto-import-resolver": "^4.3.2",
    "@tailwindcss/vite": "^4.0.14",
    "@types/webrtc": "^0.0.45",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^34.3.3",
