  "version": "1.0.0",
  "description": "TalkMe - Video Conferencing Application",
  "main": "./out/main/index.js",
  "author": "pushme",
  "homepage": "https://electron-vite.org",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "format": "prettier --write .",
